{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "SIGame",
  "version": "8.8.4",
  "identifier": "com.vladimirkhil.sigame",
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "",
    "frontendDist": "../../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "SIGame",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "maximized": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}
